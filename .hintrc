{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/name-role-value": "off",
    "typescript-config/is-valid": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "min-height: auto"
        ]
      }
    ],
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ]
  }
}